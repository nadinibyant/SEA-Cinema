{
    "version": 2,
    "builds": [{
            "src": "./app.js",
            "use": "@vercel/node"
        },
        {
            "src": "public/**",
            "use": "@vercel/static"
        },
        {
            "src": "views/**",
            "use": "@vercel/static"
        }, {
            "src": "routes/**",
            "use": "@vercel/node"
        }, {
            "src": "controllers/**",
            "use": "@vercel/node"
        }, {
            "src": "models/**",
            "use": "@vercel/node"
        }
    ],
    "routes": [{
            "src": "/(.*)",
            "dest": "/"
        },
        {
            "src": "/",
            "dest": "views/index.html"
        }
    ]
}